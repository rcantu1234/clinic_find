<p id="notice"><%= notice %></p>

<h1>Listing Statics</h1>

<table>
  <thead>
    <tr>
      <th>Index</th>
      <th>Edit</th>
      <th>Show</th>
      <th>New</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @statics.each do |static| %>
      <tr>
        <td><%= static.index %></td>
        <td><%= static.edit %></td>
        <td><%= static.show %></td>
        <td><%= static.new %></td>
        <td><%= link_to 'Show', static %></td>
        <td><%= link_to 'Edit', edit_static_path(static) %></td>
        <td><%= link_to 'Destroy', static, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Static', new_static_path %>
